<div class="panel panel-default">
  <div class="panel-heading">
    <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne">
      <h4 class="panel-title">
        <%= table.name %>
      </h4>
    </a>
  </div>
  <div id="collapseOne" class="panel-collapse collapse in">
    <div class="panel-body">
      <table class="table">
        <thead>
          <tr>
            <th>Foto</th>
            <th>Invitado</th>
          </tr>
        </thead>
        <tbody>
          <% table.attendees.each do |attendee| %>
            <tr>
              <td rowspan="2"><div class="media">
                  <a class="pull-left" href="<%= guest_path(attendee) %>">
                    <img class="media-object" src="" alt="Diego">
                  </a>
                </div></td>
              <td><a href="<%= guest_path(attendee) %>"><%= attendee.username %></a></td>
            </tr>
            <tr>
              <td>Amigo rooooool</td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
  